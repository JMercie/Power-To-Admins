{
    "name": "power-to-admins",
    "description": "My amazing LWC app",
    "version": "0.0.1",
    "author": "JMercie",
    "bugs": "https://github.com/JMercie/power-to-admins/issues",
    "type": "module",
    "dependencies": {
        "compression": "^1.7.4",
        "express": "^4.17.1",
        "helmet": "^4.6.0",
        "lwc-recipes-oss-ui-components": "^0.3.2",
        "simple-git": "^2.39.1"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.54",
        "electron": "^12.0.6",
        "electron-nightly": "^14.0.0-nightly.20210430",
        "eslint": "^7.25.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-lwc-internal": "^0.0.1",
        "fs-extra": "^8.1.0",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.4",
        "lwc-services": "^3.0.4",
        "prettier": "^2.2.1",
        "typescript": "^4.2.4"
    },
    "engines": {
        "node": ">=10.13.0",
        "npm": ">=6.4.1",
        "yarn": ">=1.9.4"
    },
    "homepage": "https://github.com/JMercie/power-to-admins",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "keywords": [
        "lwc"
    ],
    "license": "MIT",
    "lint-staged": {
        "**/*.{css,html,js,json,md,ts,yaml,yml}": [
            "prettier --write"
        ],
        "./src/**/*.js": [
            "eslint"
        ]
    },
    "repository": "https://github.com/JMercie/power-to-admins",
    "scripts": {
        "build": "lwc-services build -m production",
        "build:development": "lwc-services build",
        "lint": "eslint ./src/**/*.js",
        "prettier": "prettier --write \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "serve": "node scripts/server.js",
        "start": "electron scripts/main.js",
        "test:unit": "lwc-services test:unit",
        "test:unit:coverage": "lwc-services test:unit --coverage",
        "test:unit:debug": "lwc-services test:unit --debug",
        "test:unit:watch": "lwc-services test:unit --watch",
        "watch": "lwc-services watch"
    }
}
